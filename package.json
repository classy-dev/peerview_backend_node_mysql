{
  "name": "RESTful-API",
  "version": "0.0.1",
  "scripts": {
    "start": "node index.js",
    "db-restore": "bash restore-db.sh $HOME/db-backup/",
    "dev": "NODE_ENV=development nodemon index.js",
    "dev-win": "set NODE_ENV=development&&nodemon index.js",
    "lint": "$(npm bin)/eslint .",
    "add-hooks": "node attachments.js && cp tools/hooks/pre-commit .git/hooks && chmod +x .git/hooks/pre-commit",
    "db-sync": "node -pe \"require('./lib/db').sync({force: true}).then(process.exit)\";",
    "pm2-prod": "$(npm bin)/pm2 start index.js -o ~/LOGS/.peersviewapioutput -e ~/LOGS/.peersviewapierror --name PEERSVIEW -i"
  },
  "dependencies": {
    "@sendgrid/mail": "6.3.1",
    "MD5": "1.3.0",
    "async": "2.6.1",
    "axios": "^0.19.0",
    "body-parser": "1.18.3",
    "bunyan": "1.8.12",
    "bunyan-format": "0.2.1",
    "cloudinary": "1.11.0",
    "cors": "2.8.4",
    "cron": "1.3.0",
    "crone": "0.0.1",
    "csv": "3.1.0",
    "csv-parse": "2.5.0",
    "csvtojson": "2.0.6",
    "ejs": "2.6.1",
    "excel": "1.0.0",
    "express": "4.16.3",
    "express-route-validator": "0.2.5",
    "express-session": "1.15.6",
    "express-validator": "5.3.0",
    "file-system": "2.2.2",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.14",
    "merge": "^1.2.1",
    "moment": "2.22.2",
    "multer": "1.3.1",
    "mysql": "^2.15.0",
    "mysql2": "1.5.3",
    "nodemailer": "4.6.7",
    "passport": "0.4.0",
    "passport-facebook": "2.1.1",
    "passport-google-oauth2": "0.1.6",
    "passport-linkedin-oauth2": "1.5.0",
    "pm2": "3.0.0",
    "promise": "8.0.1",
    "pug": "2.0.3",
    "randomstring": "1.1.5",
    "request": "2.87.0",
    "requests": "0.2.2",
    "sendmail": "1.4.1",
    "sequelize": "^6.0.0",
    "shelljs": "^0.8.3",
    "uuid": "3.3.2",
    "validator": "13.7.0"
  },
  "devDependencies": {
    "eslint": "5.1.0",
    "nodemon": "1.18.2"
  }
}
